<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">


	<display-name>LoginMVCExample</display-name>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>concertList</servlet-name>
		<servlet-class>controller.ConcertListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>concertList</servlet-name>
		<url-pattern>/api/concerts</url-pattern>
	</servlet-mapping>


</web-app>